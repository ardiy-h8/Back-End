{
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": ["prettier"],
  "rules": {
    "quotes": [1, "single"],
    "no-console": 1,
    "no-dupe-keys": 2,
    "eqeqeq": [2,"smart"],
    "no-lone-blocks": 2,
    "no-multi-spaces": 1,
    "no-new-wrappers": 2,
    "no-warning-comments": 1,
    "handle-callback-err": 2,
    "camelcase": 2,
    "key-spacing": 1,
    "new-cap": 1,
    "no-multiple-empty-lines": [1,{"max":1}],
    "no-spaced-func": 1,
    "no-trailing-spaces": [1,{"skipBlankLines":false}],
    "operator-linebreak": [1,"after"],
    "space-before-blocks": [1,"always"],
    "space-infix-ops": 1,
    "space-unary-ops": [1,{"nonwords":false}],
    "spaced-comment": [1,"always",{}],
    "arrow-spacing": [1,{"before":true,"after":true}],
    "constructor-super": 2,
    "no-var": 2,
    "object-shorthand": [1,"always"],
    "prefer-const": 2
  }
}